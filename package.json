{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager --platform=darwin --arch=x64 . MyApp --ignore-node_modules/electron-*",
		"package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true",
		"create-installer-win": "node installers/windows/createinstaller.js"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^3.0.4",
    "electron-packager": "^12.2.0",
    "electron-winstaller": "^2.7.0"
  }
}
